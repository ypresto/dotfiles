[user]
	name = Yuya Tanaka
	email = yuya.presto@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/bin/env vim
[color]
	ui = auto
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	rs = reset
	rsh = reset --hard
	di = diff
	dc = diff --cached
	lo = log
	dt = difftool
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[github]
	user = yuya-presto
[push]
	default = tracking
